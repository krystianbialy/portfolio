{
  "extends": ["airbnb-base/legacy", "prettier"],
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars":"off",
      "no-unused-expressions":"off"
  },
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
  }
}